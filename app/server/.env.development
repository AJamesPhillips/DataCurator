# public / shared config
NODE_ENV=development
## Additional note, on development and test, these values must be the same as
## the PRIVATE_SERVER_CONFIG_x environment variables
SERVER_SCHEME=http
SERVER_HOST=localhost
SERVER_PORT=4000
LOG_LEVEL=5

# private server only config
PRIVATE_SERVER_CONFIG_SCHEME=http
PRIVATE_SERVER_CONFIG_HOST=127.0.0.1
PRIVATE_SERVER_CONFIG_PORT=4000
PRIVATE_SERVER_CONFIG_DB_DATABASE=worldsim_datacurator_dev_db
PRIVATE_SERVER_CONFIG_DB_USERNAME=postgres
PRIVATE_SERVER_CONFIG_DB_PASSWORD=
PRIVATE_SERVER_CONFIG_DB_ADDRESS=127.0.0.1
PRIVATE_SERVER_CONFIG_DB_PORT=5432
PRIVATE_SERVER_CONFIG_DB_DIALECT=postgres
PRIVATE_SERVER_CONFIG_LOG_DB=1
PRIVATE_SERVER_CONFIG_LOG_APP_SERVER_DIRECTORY=./log/dev/

## In staging / production this can be generated from create_env.js
PRIVATE_SERVER_CONFIG_ENCRYPTION_PASSWORD=some_really_long_password_of_90_of_more_characters_that_will_be_strong_random_because_it_is_generated_from_node_crypto_randomBytes
